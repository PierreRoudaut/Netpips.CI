#!/bin/bash
#
# - Insert netpips SuperAdmin
#
##################################

set -exu

if [ "$#" -ne 1 ]; then
    echo "./$script_name [OPENSUB_USERNAME]"
    exit 1
fi

cs=$(cat /home/netpips/netpips.staging.settings.json | jq .ConnectionStrings.Default --raw-output)

/opt/mssql-tools/bin/sqlcmd -U '' -P '' -Q "USE netpips INSERT INTO Users (Id, Email, Role) VALUES ('00000000-0000-0000-0000-000000000000', '', 'SuperAdmin')"

